    "ejs-lint": "^1.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "install": "^0.13.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "postcss-html": "^0.36.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "prettier-stylelint": "^0.4.2",
    "serve": "^11.3.2",
    "stylelint": "^13.12.0",
    "stylelint-config-recess-order": "^2.3.0",
