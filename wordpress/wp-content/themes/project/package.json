  "main": "index.js",
  "scripts": {
    "stylelint": "stylelint original/scss --fix",
    "watch:stylelint": "chokidar \"original/scss/\" -c \"npm run stylelint\" --initial",
    "watch:sass": "node-sass -r original/scss -o original/tmp_css --output-style expanded --watch",
    "watch:postcss": "postcss original/tmp_css --use autoprefixer --dir css --no-map --watch",
    "stylelintCss": "stylelint css --fix",
    "watch:stylelintCss": "chokidar \"css/\" -c \"npm run stylelintCss\" --initial",
    "all": "run-p watch:*"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "browserslist": "^4.16.3",
    "bufferutil": "^4.0.3",
